%p#notice= notice
%h1 Listing Notifications
%table
  %thead
    %tr
      %th Senderid
      %th Receiverid
      %th Targetid
      %th Message
      %th Timestamp
      %th{:colspan => "3"}
  %tbody
    - @notifications.each do |notification|
      %tr
        %td= notification.senderId
        %td= notification.receiverId
        %td= notification.targetId
        %td= notification.message
        %td= notification.timestamp
        %td= link_to 'Show', notification
        %td= link_to 'Edit', edit_notification_path(notification)
        %td= link_to 'Destroy', notification, method: :delete, data: {confirm: 'Are you sure?'}
%br/
= link_to 'New Notification', new_notification_path
