      p.cjk { font-family: "Cambria"; font-size: 12pt }
      p.ctl { font-family: "Cambria" }
      a:link { so-language: zxx }
    %body{:bgcolor => "#ffffff", :dir => "ltr", :lang => "en-US", :style => "background: #ffffff", :text => "#000000"}
    %div{:title => "header"}
