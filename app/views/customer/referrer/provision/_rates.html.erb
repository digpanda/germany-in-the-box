<% unless package_sets.count == 0 %>
<div class="provision__orders">

  <!-- ORDERS LIST -->
  <div class="provision__title">
    <%= I18n.t('referrer.provision_rates') %>
  </div>

  <div class="provision__list">
    <table>
      <thead>
        <th>
          <%= I18n.t('referrer.package_set_product') %>
        </th>
        <th>
          <%= I18n.t('referrer.rate') %>
        </th>
      </thead>
      <tbody>

        <% package_sets.each do |package_set| %>
          <% if packaget_set.referrer_rate > 0.0 %>
            <tr data-href="#">
              <td>
                <%= package_set.name %>
              </td>
              <td>
                <%= package_set.referrer_rate %>%
              </td>
            </tr>
          <% end %>
        <% end %>

      </tbody>
    </table>
  </div>

</div>

<div class="+spacer"></div>
<% end %>
